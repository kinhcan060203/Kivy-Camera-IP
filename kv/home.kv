#:import get_color_from_hex kivy.utils.get_color_from_hex
#:import Gradient kivy_gradient.Gradient
<HomeScreen>:
    name: "home_screen"
    MDScreen:
        size_hint_x: 1
        MDBoxLayout:
            spacing: dp(5)
            padding: dp(10),dp(5),dp(5),dp(5)
            orientation: 'vertical'
            MDBoxLayout:
                size_hint_y:2
                id:camera

                canvas.before:
                    Color:
                        rgba: (0,0,1,0.3)
                    Line:
                        rounded_rectangle: [self.x, self.y, self.width, self.height, 12, 12, 12, 12]  
                        width:2
            MDBoxLayout:
                size_hint_y:1
                id:log
                name: log
                canvas.before:
                    Color:
                        rgba: (0,0,1,0.3)
                    Line:
                        rounded_rectangle: [self.x, self.y, self.width, self.height, 12, 12, 12, 12]  
                        width:2

                        
            

     